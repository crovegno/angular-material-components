    "core-js": "^3.31.0",
    "highlight.js": "^11.8.0",
    "moment": "^2.29.4",
    "ngx-highlightjs": "^9.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.5.3",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~16.1.1",
